{
  "author": "AtomJump.com",
  "name": "ehr-medtech-con",
  "description": "MedTech32 config for EHR Connector Add-on for MedImage",
  "version": "0.0.7",
  "private": false,
  "license": "MIT License",
  "installCommands": {
  	 "all": [
  	  ],
  	  "win32": [
  	  	  "COPY ehrconnect.json ..\\ehrconnect\\config\\ehrconnect.json", 
  	      "if not exist C:\\odbc-interbase\\ ( MKDIR C:\\odbc-interbase )",
  	      "COPY .\\odbc\\odbcibinstall.exe C:\\odbc-interbase\\odbcibinstall.exe",
  	      "C:\\odbc-interbase\\odbcibinstall.exe /S"
  	  ],
  	  "win64": [
  	  	  "COPY ehrconnect.json ..\\ehrconnect\\config\\ehrconnect.json", 
  	      "if not exist C:\\odbc-interbase\\ ( MKDIR C:\\odbc-interbase )",
  	      "COPY .\\odbc\\odbcibinstall.exe C:\\odbc-interbase\\odbcibinstall.exe",
  	      "C:\\odbc-interbase\\odbcibinstall.exe /S"
  	  ],
  	  "unix": [
  	  ],
  	  "mac": [
  	  	 
  	  ]
  },
   "uninstallCommands": {
  	 "all": [
  	  ],
  	  "win32": [
  	  	"del C:\\odbc-interbase\\odbcibinstall.exe",
  	  	"rmdir C:\\odbc-interbase\\"
  	  ],
  	  "win64": [
  	  	 "del C:\\odbc-interbase\\odbcibinstall.exe",
  	  	 "rmdir C:\\odbc-interbase\\"
  	  ],
  	  "unix": [
  	  ],
  	  "mac": [
  	  ]
  }
}
